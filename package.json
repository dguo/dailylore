{
  "name": "dailylore",
  "version": "0.1.0",
  "description": "Lightweight news headlines aggregator",
  "main": "index.js",
  "private": true,
  "scripts": {
    "lint": "eslint .; sass-lint -v -q; ./verifyMinification.sh; exit 0",
    "lint:ci": "eslint . && sass-lint -v -q",
    "build": "webpack --progress --colors --watch",
    "build:prod": "webpack --progress --colors --optimize-minimize",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dguo/dailylore.git"
  },
  "keywords": [
    "news",
    "headlines",
    "aggregator"
  ],
  "author": "Danny Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dguo/dailylore/issues"
  },
  "homepage": "https://github.com/dguo/dailylore#readme",
  "dependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.14.0",
    "css-loader": "0.25.0",
    "debug": "2.2.0",
    "eslint": "3.5.0",
    "materialize-css": "0.97.7",
    "node-sass": "3.9.3",
    "sass-lint": "1.9.1",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "whatwg-fetch": "1.0.0"
  }
}
